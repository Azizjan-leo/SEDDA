<Window x:Class="SEDDA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEDDA"
        mc:Ignorable="d"
        Title="MainWindow" Height="130" Width="208" >

    <Grid Margin="0,0,0,0">
        <TextBox x:Name="X" HorizontalAlignment="Left" Height="23" Margin="55,4,0,0" TextWrapping="Wrap" Text="600" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Y" HorizontalAlignment="Left" Height="23" Margin="55,32,0,0" TextWrapping="Wrap" Text="600" VerticalAlignment="Top" Width="120"/>
        <Button Content="Draw" HorizontalAlignment="Left" Margin="55,60,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click"/>
        <Label Content="Height:" HorizontalAlignment="Left" Margin="7,4,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.938,0.423"/>
        <Label Content="Width:" HorizontalAlignment="Left" Margin="10,29,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
